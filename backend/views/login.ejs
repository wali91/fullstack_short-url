<%- include('partials/_header') %>

<div class="container">
  <h3>Login</h3>
  <form method="POST" action="/users/login" >
		<div class="form-group">
			<label for="email">Email:</label>
			<input type="text" style="width: 40%;" class="form-control" id="email" name="email">
    </div>
		<div class="form-group">
			<label for="password">Password:</label>
			<input type="password" style="width: 40%;" class="form-control" id="password" name="password"></input>
    </div>
    <input type="submit" class="btn btn-primary" value="Login">
  </form>

</div>